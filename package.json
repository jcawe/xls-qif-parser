{
  "name": "xlsqifparser",
  "version": "2.0.0",
  "description": "xls to qif converter",
  "main": "./dist/main.js",
  "dependencies": {
    "moment": "^2.29.2",
    "xlsx": "^0.17.0"
  },
  "devDependencies": {
    "electron": "^18.3.7",
    "electron-builder": "^22.8.1",
    "typescript": "^3.9.7"
  },
  "build": {
    "files": {
      "filter": [
        "dist/**",
        "views/**"
